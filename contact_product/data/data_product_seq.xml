                <field name="prefix">SS</field>
                <field name="padding">1</field>
            </record>

            <record id="seq_id" model="ir.sequence">
                <field name="name">Product Sequence</field>
                <field name="code">category.sequence</field>
                <field name="number_next_actual">001</field>
                <field name="prefix">ProductSequence</field>
                <field name="padding">1</field>
            </record>

    </data>
    
</odoo>
