<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <template id="report_saleorder_document_inherit_2_discount_management" inherit_id="sale.report_saleorder_document"> 
            <xpath expr="//th[@name='th_discount']" position="after">
                <th name="Second Discount">
                    <span>2nd Disc.%</span>
                </th>
            </xpath>
            <xpath expr="//td[@t-if='display_discount']" position="after">
                <td name="display_discount2">
                    <span t-field="line.secound_discount"/>
                </td> 
            </xpath>
        </template>

    </data>
    

</odoo>
