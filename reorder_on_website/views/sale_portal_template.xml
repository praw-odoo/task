<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="template_document_inherit" inherit_id="sale.portal_my_orders">
        <xpath expr="//tr/th[4]" position="after">
            <th class="text-right">Reorder</th>
        </xpath>
        <xpath expr="//tr/td[4]" position="after">
            <td class="text-right">
                <button name="action_reorder" type="button" class="btn-primary" >Reorder</button>
            </td>
        </xpath>
    </template>

</odoo>
