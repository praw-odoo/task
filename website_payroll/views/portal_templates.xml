<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="portal_my_home_pay" name="PlaySlips" customize_show="True" inherit_id="portal.portal_my_home" priority="25">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">PlaySlips</t>
                <t t-set="url" t-value="'/my/payslip'"/>
                <t t-set="placeholder_count" t-value="'slip_count'"/>
            </t>
        </xpath>
    </template>

</odoo>
